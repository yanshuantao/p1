* {
	margin: 0;
	padding: 0;
}

img {
	border: none;
	border: 0;
}

.tb01 {
	border-collapse: collapse;
}

.inbspan {
	display: inline-block;
	margin-right: 26px;
	font-size: 12px;
	color: #737373;
	padding:3px;
}

.inps {
	width: 216px;
	height: 16px;
	padding: 2px;
	line-height: 16px;
	border: 1px solid #c8c7c7;
	margin-left: 10px;
}

.tb01 td,.tb01 th {
	padding: 8px 0;
	border: 1px solid #ebebeb;
	text-align: center;
}

.tb01 th {
	background: #fafafa;
}

.tb01 td {
	color: #717171;
	font-size: 12px;
}

.sq {
	display: inline-block;
	height: 15px;
	line-height: 15px;
	color: #000000;
	font-size: 12px;
	text-align: center;
	border: 1px solid #bdd8e9;
	text-decoration: none;
	background: url(images/retx01.jpg) repeat-x;
	padding: 0 5px;
	border-radius: 2px;
}

.searchbtn {
	background: url(../images/ss.jpg) no-repeat;
	vertical-align: middle;
	width: 66px;
	height: 24px;
	font-size: 0;
	line-height: 0;
	overflow: hidden;
	border: none;
	cursor: pointer;
}

.updatebtn {
	background: url(../images/xg.jpg) no-repeat;
	vertical-align: middle;
	width: 66px;
	height: 24px;
	font-size: 0;
	line-height: 0;
	overflow: hidden;
	border: none;
	cursor: pointer;
}

.addbtn {
	background: url(../images/add.jpg) no-repeat;
	vertical-align: middle;
	width: 66px;
	height: 24px;
	font-size: 0;
	line-height: 0;
	overflow: hidden;
	border: none;
	cursor: pointer;
}

.wrap{
	color: #16487F; font-size: 12px;
}


.block{
	display:block;
}

.ltd{
	width:100px;
	text-align:right;
}

